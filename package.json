{
  "name": "WeatherTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant release",
    "android:build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:build:release": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && rm -rf android/app/src/main/res/drawable-* && rm -rf android/app/src/main/res/raw/* && cd android && ./gradlew assembleRelease && cd ..",
    "android:install:debug": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:podinstall": "cd ios && pod install && cd ..",
    "ios:build": "react-native bundle --entry-file='index.js' --bundle-output='./ios/SitePod/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "ios:build:bundle": "cd ios && xcodebuild -sdk iphonesimulator -workspace SitePod.xcworkspace/ -scheme SitePod -configuration Release && cd ..",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "react-devtools": "react-devtools"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.12",
    "@react-navigation/native-stack": "^6.9.20",
    "react": "18.2.0",
    "react-native": "0.73.5",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-reanimated": "^3.7.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@tsconfig/react-native": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.60",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-expo": "^10.0.1",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-devtools": "^5.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
